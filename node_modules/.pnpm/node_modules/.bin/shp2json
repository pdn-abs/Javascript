#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/pdn/dev/Javascript/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/pdn/dev/Javascript/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../shapefile/bin/shp2json" "$@"
else
  exec node  "$basedir/../shapefile/bin/shp2json" "$@"
fi
